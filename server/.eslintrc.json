{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "semi": "error",
        "quotes": ["error", "single"],
        "comma-dangle": ["error", "never"],
        "class-methods-use-this": "off",
        "import/extensions": "off",
        "no-underscore-dangle": "off"
    }
}
